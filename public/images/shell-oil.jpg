<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#f3f4f6"/>
  <g transform="translate(200,150)">
    <!-- Oil bottle -->
    <rect x="-30" y="-90" width="60" height="180" fill="#fbbf24" rx="8"/>
    <!-- Shell logo -->
    <circle cx="0" cy="-60" r="25" fill="#dc2626"/>
    <text x="0" y="-55" text-anchor="middle" font-family="Arial" font-size="20" font-weight="bold" fill="#ffffff">S</text>
    <!-- Oil type label -->
    <rect x="-20" y="-20" width="40" height="30" fill="#ffffff" rx="3"/>
    <text x="0" y="-5" text-anchor="middle" font-family="Arial" font-size="12" fill="#1f2937">5W-30</text>
    <!-- Volume indicator -->
    <rect x="-25" y="20" width="50" height="100" fill="#f59e0b" rx="5"/>
    <text x="0" y="80" text-anchor="middle" font-family="Arial" font-size="14" fill="#ffffff">4L</text>
  </g>
  <text x="200" y="280" text-anchor="middle" font-family="Arial" font-size="16" fill="#6b7280">Shell Oil</text>
</svg>
