<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#f3f4f6"/>
  <g transform="translate(200,150)">
    <!-- Spark plug body -->
    <rect x="-15" y="-60" width="30" height="120" fill="#6b7280" rx="5"/>
    <!-- Spark plug tip -->
    <rect x="-8" y="60" width="16" height="20" fill="#374151"/>
    <!-- Spark plug threads -->
    <rect x="-20" y="-40" width="40" height="80" fill="#9ca3af" rx="5"/>
